{
  "name": "postgres-source-joined_tables",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "tasks.max": "1",
    "slot.name": "debezium_joined_tables",
    "publication.name": "debezium_joined_tables",
    "database.hostname": "${PSQL_HOST}",
    "database.port": "${PSQL_PORT}",
    "database.user": "${PSQL_USERNAME}",
    "database.password": "${PSQL_PASSWORD}",
    "database.dbname" : "${PSQL_DATABASE}",
    "database.server.name": "abc",
    "table.include.list": "public.cross_reference,public.author,public.mod_corpus_association,public.obsolete_reference_curie,public.reference_mod_referencetype,public.topic_entity_tag,public.workflow_tag,public.reference_relation,public.mesh_detail",
    "database.history.kafka.bootstrap.servers": "dbz_kafka:9092",
    "decimal.handling.mode" : "string",
    "transforms": "filterHeartbeats,unwrap",
    "transforms.filterHeartbeats.type": "org.apache.kafka.connect.transforms.Filter",
    "transforms.filterHeartbeats.predicate": "isHeartbeat",
    "predicates": "isHeartbeat",
    "predicates.isHeartbeat.type": "org.apache.kafka.connect.transforms.predicates.TopicNameMatches",
    "predicates.isHeartbeat.pattern": "__debezium-heartbeat.joined_tables.*",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.unwrap.operation.header": "true",
    "plugin.name": "pgoutput",
    "database.tcpKeepAlive": "true",
    "database.tcpUserTimeout": "30000",
    "database.query.timeout.ms": "300000",
    "database.connectTimeout": "30000",
    "database.socketTimeout": "30000",
    "heartbeat.interval.ms": "10000",
    "heartbeat.action.query": "SELECT 1",
    "heartbeat.topics.prefix": "__debezium-heartbeat.joined_tables",
    "slot.stream.params.status_interval": "10s",
    "poll.interval.ms": "1500",
    "max.queue.size": "32768",
    "max.batch.size": "4096",
    "errors.tolerance": "all",
    "errors.deadletterqueue.topic.name": "debezium.errors.joined",
    "errors.deadletterqueue.topic.replication.factor": "1",
    "errors.deadletterqueue.context.headers.enable": "true",
    "errors.log.enable": "true",
    "errors.log.include.messages": "true"
  }
}
