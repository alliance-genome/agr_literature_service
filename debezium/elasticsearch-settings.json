{
  "settings": {
    "number_of_shards": "1",
    "number_of_replicas": "0",
    "max_ngram_diff": "20",
    "max_result_window": 1000000,
    "analysis": {
      "filter": {
        "ngram_filter": {
          "type": "edge_ngram",
          "min_gram": "1",
          "max_gram": "20"
        }
      },
      "char_filter": {
        "LanguageFilter": {
          "type": "mapping",
          "mappings": [
            "eng=>English",
            "Czech and English=>English",
            "English and Dutch=>English",
            "English and French=>English",
            "English and Hebrew=>English",
            "English and Japanese=>English",
            "English and Russian=>English",
            "English and Spanish=>English",
            "French and English=>English",
            "German and English=>English",
            "Hungarian and English=>English",
            "Icelandic and English=>English",
            "Italian and English=>English",
            "Portuguese and English=>English",
            "Russian and English=>English",
            "Spanish and English=>English",
            "Ukrainian and English =>English",
            "Hungarian and German=>Other",
            "Latin and German=>Other",
            "Latin and Hungarian=>Other",
            "Portuguese and French=>Other",
            "Russian and German=>Other",
            "Afrikaans=>Other",
            "Arabic=>Other",
            "Belarussian=>Other",
            "Bulgarian=>Other",
            "Catalan=>Other",
            "Chinese=>Other",
            "Czech=>Other",
            "Danish=>Other",
            "Dutch=>Other",
            "Estonian=>Other",
            "Farsi=>Other",
            "Finnish=>Other",
            "Flemish=>Other",
            "French=>Other",
            "German=>Other",
            "Greek=>Other",
            "Hebrew=>Other",
            "Hungarian=>Other",
            "Icelandic=>Other",
            "Indonesian=>Other",
            "Italian=>Other",
            "Japanese=>Other",
            "Jugoslavian=>Other",
            "Korean=>Other",
            "Latin=>Other",
            "Lithuanian=>Other",
            "Malayalam=>Other",
            "Norwegian=>Other",
            "Polish=>Other",
            "Portuguese=>Other",
            "Romanian=>Other",
            "Russian=>Other",
            "Serbo-Croat(Roman)=>Other",
            "Serbo-Croat=>Other",
            "Slovak=>Other",
            "Slovenian=>Other",
            "Spanish=>Other",
            "Swedish=>Other",
            "Turkish=>Other",
            "Ukrainian=>Other",
            "chi=>Other",
            "cze=>Other",
            "dan=>Other",
            "dut=>Other",
            "fin=>Other",
            "fre=>Other",
            "ger=>Other",
            "heb=>Other",
            "hrv=>Other",
            "hun=>Other",
            "ice=>Other",
            "ita=>Other",
            "jpn=>Other",
            "kor=>Other",
            "mul=>Other",
            "nor=>Other",
            "pol=>Other",
            "por=>Other",
            "rum=>Other",
            "rus=>Other",
            "slo=>Other",
            "spa=>Other",
            "srp=>Other",
            "swe=>Other",
            "tur=>Other",
            "ukr=>Other",
            "und=>Other"
          ]
        },
        "EmptyStringFilter" : {
          "type": "pattern_replace",
          "pattern": "^$",
          "replacement": "English"
        }
      },
      "normalizer": {
        "sortNormalizer": {
          "filter": [
            "asciifolding",
            "lowercase"
          ],
          "type": "custom"
        },
        "languageNormalizer": {
          "char_filter" : ["LanguageFilter","EmptyStringFilter"],
          "type": "custom"
        }
      },
      "analyzer": {
        "authorNameAnalyzer": {
          "type": "custom",
          "tokenizer": "whitespace",
          "filter": [
            "asciifolding",
            "lowercase"
          ]
        },	    
        "autocompleteAnalyzer": {
          "filter": [
            "asciifolding",
            "lowercase"
          ],
          "type": "custom",
          "tokenizer": "ngram_tokenizer"
        },
        "autocompleteSearchAnalyzer": {
          "filter": [
            "asciifolding",
            "lowercase"
          ],
          "type": "custom",
          "tokenizer": "ngram_tokenizer"
        },
        "default": {
          "type": "custom",
          "tokenizer": "custom_pattern_tokenizer",
          "filter": [
            "lowercase"
          ]
        },
        "default_search": {
          "type": "custom",
          "tokenizer": "custom_pattern_tokenizer",
          "filter": [
            "lowercase"
          ]
        },
        "lowercasespaceanalyzer": {
          "type": "custom",
          "tokenizer": "custom_pattern_tokenizer",
          "filter": [
            "lowercase"
          ]
        }
      },
      "tokenizer": {
        "ngram_tokenizer": {
          "token_chars": [
            "letter",
            "digit"
          ],
          "min_gram": "1",
          "type": "edge_ngram",
          "max_gram": "20"
        },
        "custom_pattern_tokenizer": {
          "type": "pattern",
          "pattern": "\\p{Punct}{0,1}\\s+"
        }
      }
    }
  },
  "mappings": {
    "properties": {
      "short_citation": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "authors": {
        "properties": {
          "name": {
            "type": "text",
            "analyzer": "authorNameAnalyzer",
            "fields": {
              "keyword": {
                "type": "keyword",
                "normalizer": "sortNormalizer",
                "ignore_above": 256
              }
            }
          },
          "orcid": {
            "type": "text",
            "analyzer": "autocompleteAnalyzer",
            "search_analyzer": "autocompleteSearchAnalyzer",
            "fields": {
              "keyword": {
                "type": "keyword",
                "normalizer": "sortNormalizer",
                "ignore_above": 256
              }
            }
          }
        }
      },
      "citation": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "curie": {
        "type": "text",
        "analyzer": "autocompleteAnalyzer",
        "search_analyzer": "autocompleteSearchAnalyzer",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "title": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "volume": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "date_published": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "date_published_start": {
        "type": "date",
        "format": "date_optional_time||epoch_millis",
        "null_value": null,
        "ignore_malformed": true
      },
      "date_published_end": {
        "type": "date",
        "format": "date_optional_time||epoch_millis",
        "null_value": null,
        "ignore_malformed": true
      },
      "date_created": {
        "type": "date",
        "format": "date_optional_time||epoch_millis",
        "null_value": null,
        "ignore_malformed": true
      },
      "page_range": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "abstract": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "publisher": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "pubmed_types": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "issue_name": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "category": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "pubmed_publication_status": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "date_arrived_in_pubmed": {
        "type": "date",
        "format": "date_optional_time||epoch_millis",
        "null_value": null,
        "ignore_malformed": true
      },
      "date_last_modified_in_pubmed": {
        "type": "date",
        "format": "date_optional_time||epoch_millis",
        "null_value": null,
        "ignore_malformed": true
      },
      "keywords": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "cross_references.curie": {
        "type": "text",
        "analyzer": "autocompleteAnalyzer",
        "search_analyzer": "autocompleteSearchAnalyzer",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "sortNormalizer",
            "ignore_above": 256
          }
        }
      },
      "language": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "normalizer": "languageNormalizer",
            "null_value": "English",
            "ignore_above": 256
          }
        }
      },
      "cross_references.is_obsolete": {
        "type": "boolean"
      },
      "workflow_tags": {
        "type": "nested",
        "properties": {
          "workflow_tag_id": {
            "type": "text",
            "analyzer": "autocompleteAnalyzer",
            "search_analyzer": "autocompleteSearchAnalyzer",
            "fields": {
              "keyword": {
                "type": "keyword",
                "normalizer": "languageNormalizer",
                "ignore_above": 256
              }
            }
          },
          "mod_abbreviation": {
            "type": "keyword",
            "normalizer": "sortNormalizer"
          }
        }
      },
      "topic_entity_tags": {
        "type": "nested",
        "properties": {
          "topic": {
            "type": "text",
            "analyzer": "autocompleteAnalyzer",
            "search_analyzer": "autocompleteSearchAnalyzer",
            "fields": {
              "keyword": {
                "type": "keyword",
                "normalizer": "sortNormalizer",
                "ignore_above": 256
              }
            }
          },
          "entity_type": {
            "type": "text",
            "analyzer": "autocompleteAnalyzer",
            "search_analyzer": "autocompleteSearchAnalyzer",
            "fields": {
              "keyword": {
                "type": "keyword",
                "normalizer": "sortNormalizer",
                "ignore_above": 256
              }
            }
          },
          "entity": {
            "type": "text",
            "analyzer": "autocompleteAnalyzer",
            "search_analyzer": "autocompleteSearchAnalyzer",
            "fields": {
              "keyword": {
