                "normalizer": "sortNormalizer",
                "ignore_above": 256
              }
            }
          },
          "species": {
            "type": "text",
            "analyzer": "autocompleteAnalyzer",
            "search_analyzer": "autocompleteSearchAnalyzer",
            "fields": {
              "keyword": {
                "type": "keyword",
                "normalizer": "sortNormalizer",
                "ignore_above": 256
              }
            }
          },
          "negated": {
            "type": "boolean"
          },
          "novel_entity_data": {
            "type": "boolean"
          },
	  "data_provider": {
            "type": "keyword",
            "normalizer": "sortNormalizer"
          },
          "source_method": {
            "type": "text",
            "analyzer": "autocompleteAnalyzer",
            "search_analyzer": "autocompleteSearchAnalyzer",
            "fields": {
              "keyword": {
                "type": "keyword",
                "normalizer": "sortNormalizer",
                "ignore_above": 256
              }
            }
          },
          "source_evidence_assertion": {
            "type": "text",
            "analyzer": "autocompleteAnalyzer",
            "search_analyzer": "autocompleteSearchAnalyzer",
            "fields": {
              "keyword": {
                "type": "keyword",
                "normalizer": "sortNormalizer",
                "ignore_above": 256
              }
            }
          }
        }
      }
    }
  }
}
